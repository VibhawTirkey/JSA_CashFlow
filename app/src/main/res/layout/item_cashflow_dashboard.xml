<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardCornerRadius="30dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.cardview.widget.CardView
            android:id="@+id/container1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            app:cardCornerRadius="10dp"
            app:cardElevation="6dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <View
                    android:id="@+id/view"
                    android:layout_width="6dp"
                    android:layout_height="50dp"
                    android:background="@color/toolbar_blue"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/label_gp_percent"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="GP %"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:fontFamily="@font/nunito_bold"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/label_np_percent"/>

                <TextView
                    android:id="@+id/label_np_percent"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="NP %"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:fontFamily="@font/nunito_bold"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toEndOf="@id/label_gp_percent"
                    app:layout_constraintEnd_toStartOf="@id/label_roi_percent"/>

                <TextView
                    android:id="@+id/label_roi_percent"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ROI %"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:fontFamily="@font/nunito_bold"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toEndOf="@id/label_np_percent"
                    app:layout_constraintEnd_toStartOf="@id/label_inventory_turn"/>

                <TextView
                    android:id="@+id/label_inventory_turn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Inventory\nTurn"
                    android:layout_margin="5dp"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:fontFamily="@font/nunito_bold"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toEndOf="@id/label_roi_percent"
                    app:layout_constraintEnd_toStartOf="@id/label_current_ratio"/>

                <TextView
                    android:id="@+id/label_current_ratio"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Current\nRatio"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:fontFamily="@font/nunito_bold"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toEndOf="@id/label_inventory_turn"
                    app:layout_constraintEnd_toStartOf="@id/label_quick_ratio"/>

                <TextView
                    android:id="@+id/label_quick_ratio"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Quick\nRatio"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:fontFamily="@font/nunito_bold"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toEndOf="@id/label_current_ratio"
                    app:layout_constraintEnd_toEndOf="parent"/>

            </androidx.constraintlayout.widget.ConstraintLayout>


        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@id/container1">



        </androidx.cardview.widget.CardView>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>